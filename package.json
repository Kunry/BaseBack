{
  "name": "base-back",
  "version": "1.0.0",
  "description": "template back",
  "main": "src/app.ts",
  "scripts": {
    "test:coverage": "ENV_FILE=.env.test jest --config config/jest.json --collectCoverage",
    "start": "NODE_ENV=production node -r module-alias/register dist/app.js",
    "dev": "NODE_ENV=development nodemon",
    "dev:prod": "NODE_ENV=production nodemon",
    "build": "rm -rf dist && npx tsc --build tsconfig.prod.json",
    "serve": "yarn run build && NODE_ENV=production node -r module-alias/register dist/app.js",
    "seeds:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register --files ./src/seeds --env=development",
    "seeds:prod": "NODE_ENV=production ts-node -r tsconfig-paths/register --files ./src/seeds --env=production",
    "prettier": "prettier  --config .prettierrc --write src/**/*.ts",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "template",
    "back"
  ],
  "_moduleAliases": {
    "@constants": "dist/constants",
    "@utils": "dist/utils",
    "@log": "dist/loaders/log.loader",
    "@errors": "dist/errors"
  },
  "author": "Tailor-Hub",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/joi": "^17.2.3",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.19",
    "@types/pino": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2",
    "helmet": "^5.0.2",
    "http": "^0.0.1-security",
    "joi": "^17.6.0",
    "module-alias": "^2.2.2",
    "mongodb-memory-server": "^8.4.0",
    "mongoose": "^6.2.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-mocks-http": "^1.11.0",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.1",
    "tslib": "^2.3.1",
    "winston": "^3.6.0"
  }
}
